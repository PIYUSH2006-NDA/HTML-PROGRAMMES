//write a loop to print number from 1 to 5,along with thier square and cube 
#include <stdio.h>
int main()
{
    int a,s,c;
    printf("enter theh number:-");
    scanf("%d",&a);
    for(a=1;a<=5;a+=1)
    {
        s=a*a;
        c=a*a*a;
        printf("\nnumber=%d,the square of the number is:- %d and the cube of the number is:-%d ",a,s,c);
    }
    return 0;
}
